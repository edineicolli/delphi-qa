program aprender;

uses
  Vcl.Forms,
  Main in '..\src\Main.pas' {Form1},
  uFrmBase in '..\src\uFrmBase.pas' {FrmBase};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.CreateForm(TFrmBase, FrmBase);
  Application.Run;
end.
